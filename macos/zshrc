ZSH_TMUX_AUTOSTART='false'

# Definições para Android Emulator (AVD)
alias emulator="~/Library/Android/sdk/emulator/emulator"
alias hosts="code /etc/hosts"
alias c="code ."

# Development variables
export WORKSPACE_PATH=~/workspace
export IDF_PATH=$WORKSPACE_PATH/esp/ESP8266_RTOS_SDK
export XTENSA_PATH=$WORKSPACE_PATH/esp/xtensa-lx106-elf
export PATH="$PATH:$IDF_PATH/tools:$XTENSA_PATH/bin:/usr/local/opt/ccache/libexec"

test -e "${HOME}/.iterm2_shell_integration.zsh" && source "${HOME}/.iterm2_shell_integration.zsh"

