# key[F4]        = '^[[[D'
# key[F5]        = '^[[[E'
# key[F6]        = '^[[17~'
# key[F7]        = '^[[18~'
# key[F8]        = '^[[19~'
# key[F9]        = '^[[20~'
# key[F10]       = '^[[21~'
# key[F11]       = '^[[23~'
# key[F12]       = '^[[24~'

bindkey -s '^[[17~' 'git pull \C-m'
bindkey -s '^[[18~' 'git status \C-m'
bindkey -s '^[[19~' 'git add --patch \C-m'
bindkey -s '^[[20~' 'git commit \C-m'
bindkey -s '^[[21~' 'git push \C-m'



